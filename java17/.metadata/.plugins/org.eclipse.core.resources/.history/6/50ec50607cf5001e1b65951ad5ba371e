package suda;

public class CheckForVowel {
	
	public static void main(String[] args) {
		char character='e';
		switch (character) {
		case 'i': 
			System.out.println("It is a vowel");
			break;
		case 'u': 
			System.out.println("It is a vowel");
			break;
		case '0': 
			System.out.println("It is a vowel");
			break;
		case 'a': 
			System.out.println("It is a vowel");
			break;
		case 'e': 
			System.out.println("It is a vowel");
			break;
			
		case 'I': 
			System.out.println("It is a vowel");
			break;
		case 'U': 
			System.out.println("It is a vowel");
			break;
		case 'O': 
			System.out.println("It is a vowel");
			break;
		case 'A': 
			System.out.println("It is a vowel");
			break;
		case 'E': 
			System.out.println("It is a vowel");
			break;
			
		default:
			System.out.println("It is a consonate");
		}
	}

}
